{
  "compilerOptions": {
    "resolveJsonModule": true,
    "target": "es6",
    "module": "commonjs",
    // "lib": [],
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "src"
  },
  /* Exclude client react folder */
  "exclude": [
    "migrations",
    "seeds",
    "build"
  ]
}
